<?xml version="1.0" encoding="UTF-8"?>
<zend-config>
    <integration_name>
        <title>接口名称</title>
        <content>UCenter系统整合</content>
        <name_id>integration_name</name_id>
        <input_type>text</input_type>
        <class>span2</class>
    </integration_name>
    <ucenter_app_id>
        <title>UCenter应用ID</title>
        <content></content>
        <name_id>app_id</name_id>
        <input_type>text</input_type>
        <class>span2</class>
    </ucenter_app_id>
    <ucenter_key>
        <title>UCenter通信密钥</title>
        <content></content>
        <name_id>uc_key</name_id>
        <input_type>text</input_type>
        <class>span6</class>
    </ucenter_key>
    <ucenter_api>
        <title>UCenter访问地址</title>
        <content>http://</content>
        <name_id>uc_api</name_id>
        <input_type>text</input_type>
        <class>span6</class>
    </ucenter_api>
    <ucenter_ip>
        <title>UCenter IP地址</title>
        <content></content>
        <name_id>uc_ip</name_id>
        <input_type>text</input_type>
        <class>span6</class>
    </ucenter_ip>
    <ucenter_dbhost>
        <title>UCenter数据库地址</title>
        <content></content>
        <name_id>uc_dbhost</name_id>
        <input_type>text</input_type>
        <class>span6</class>
    </ucenter_dbhost>
    <ucenter_dbname>
        <title>UCenter数据库名称</title>
        <content></content>
        <name_id>uc_dbname</name_id>
        <input_type>text</input_type>
        <class>span2</class>
    </ucenter_dbname>
    <ucenter_dbuser>
        <title>UCenter数据库用户名</title>
        <content></content>
        <name_id>uc_dbuser</name_id>
        <input_type>text</input_type>
        <class>span2</class>
    </ucenter_dbuser>
    <ucenter_dbpasswd>
        <title>UCenter数据库密码</title>
        <content></content>
        <name_id>uc_dbpasswd</name_id>
        <input_type>text</input_type>
        <class>span2</class>
    </ucenter_dbpasswd>
    <ucenter_state>
        <title>接口状态</title>
        <content>
            <radio_1>
                <name>开启</name>
                <value>1</value>
            </radio_1>
            <radio_2>
                <name>关闭</name>
                <value>0</value>
            </radio_2>
        </content>
        <name_id>ucenter_state</name_id>
        <input_type>radio</input_type>
        <checked>0</checked>
    </ucenter_state>
    <editaction>ucenter</editaction>
</zend-config>
